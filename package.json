{
  "name": "@travi/redux-fetch-middleware",
  "license": "MIT",
  "description": "redux middleware to enable async data fetching as the result of a dispatched action",
  "version": "0.1.0",
  "main": "lib/middleware.cjs.js",
  "module": "lib/middleware.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/redux-fetch-middleware.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "async"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/redux-fetch-middleware/issues"
  },
  "homepage": "https://github.com/travi/redux-fetch-middleware#readme",
  "devDependencies": {
    "@travi/any": "0.8.0",
    "@travi/eslint-config-travi": "0.3.0",
    "@travi/ioc": "1.0.0",
    "babel-cli": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.12.0",
    "greenkeeper-postpublish": "1.0.1",
    "husky": "0.11.9",
    "lodash": "4.17.2",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "rollup": "0.37.0",
    "rollup-plugin-babel": "2.6.1",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "validate-commit-msg": "2.8.2"
  },
  "peerDependencies": {
    "@travi/ioc": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
