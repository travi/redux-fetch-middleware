{
  "name": "@travi/redux-fetch-middleware",
  "license": "MIT",
  "description": "redux middleware to enable async data fetching as the result of a dispatched action",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    "Trevor Richardson <npm@trevorrichardson.me> (https://trevorrichardson.me)"
  ],
  "repository": "travi/redux-fetch-middleware",
  "bugs": "https://github.com/travi/redux-fetch-middleware/issues",
  "homepage": "https://github.com/travi/redux-fetch-middleware#readme",
  "main": "lib/middleware.cjs.js",
  "module": "lib/middleware.es.js",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:peer": "npm ls >/dev/null",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "generate:md": "remark . --output",
    "preversion": "npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "async"
  ],
  "devDependencies": {
    "@babel/register": "7.21.0",
    "@travi/any": "2.1.8",
    "@travi/babel-preset": "3.0.96",
    "@travi/eslint-config": "1.0.86",
    "@travi/eslint-config-mocha": "1.0.19",
    "@travi/ioc": "1.1.1",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.4.25",
    "cz-conventional-changelog": "3.3.0",
    "globstar": "1.0.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "5.0.0",
    "remark-toc": "8.0.1",
    "rollup": "3.19.1",
    "rollup-plugin-babel": "4.4.0",
    "sinon": "15.0.2"
  },
  "peerDependencies": {
    "@travi/ioc": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "milliseconds": "1.0.3",
    "timeout-as-promise": "1.0.0"
  }
}
