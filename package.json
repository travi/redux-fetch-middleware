{
  "name": "@travi/redux-fetch-middleware",
  "license": "MIT",
  "description": "redux middleware to enable async data fetching as the result of a dispatched action",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/redux-fetch-middleware",
  "bugs": "https://github.com/travi/redux-fetch-middleware/issues",
  "homepage": "https://github.com/travi/redux-fetch-middleware#readme",
  "main": "lib/middleware.cjs.js",
  "module": "lib/middleware.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "semantic-release": "semantic-release",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "async"
  ],
  "devDependencies": {
    "@travi/any": "1.7.1",
    "@travi/eslint-config-travi": "1.6.7",
    "@travi/ioc": "1.0.0",
    "babel-cli": "6.26.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.2.23",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.2",
    "husky": "0.15.0-rc.2",
    "lodash": "4.17.4",
    "markdownlint-cli": "0.7.0",
    "mocha": "5.0.0",
    "npm-run-all": "4.1.2",
    "rollup": "0.55.3",
    "rollup-plugin-babel": "3.0.3",
    "semantic-release": "12.4.1",
    "sinon": "4.2.2"
  },
  "peerDependencies": {
    "@travi/ioc": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
