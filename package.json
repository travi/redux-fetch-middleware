{
  "name": "@travi/redux-fetch-middleware",
  "license": "MIT",
  "description": "redux middleware to enable async data fetching as the result of a dispatched action",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/redux-fetch-middleware",
  "bugs": "https://github.com/travi/redux-fetch-middleware/issues",
  "homepage": "https://github.com/travi/redux-fetch-middleware#readme",
  "main": "lib/middleware.cjs.js",
  "module": "lib/middleware.es.js",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s lint:* tests:*",
    "build": "rollup -c",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "semantic-release": "semantic-release",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "async"
  ],
  "devDependencies": {
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "1.6.14",
    "@travi/ioc": "1.0.0",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-travi": "^1.3.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "commitlint-config-travi": "1.2.30",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "^1.0.0-rc.6",
    "markdownlint-cli": "0.10.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "rollup": "0.60.1",
    "rollup-plugin-babel": "3.0.4",
    "semantic-release": "^15.4.1",
    "sinon": "^6.0.0"
  },
  "peerDependencies": {
    "@travi/ioc": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "milliseconds": "1.0.3",
    "timeout-as-promise": "1.0.0"
  }
}
