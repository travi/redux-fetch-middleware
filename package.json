{
  "name": "@travi/redux-fetch-middleware",
  "description": "redux middleware to enable async data fetching as the result of a dispatched action",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^16.14 || >=18"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    "Trevor Richardson <npm@trevorrichardson.me> (https://trevorrichardson.me)"
  ],
  "repository": "travi/redux-fetch-middleware",
  "bugs": "https://github.com/travi/redux-fetch-middleware/issues",
  "homepage": "https://github.com/travi/redux-fetch-middleware#readme",
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "async"
  ],
  "main": "./lib/middleware.js",
  "module": "./lib/middleware.mjs",
  "exports": {
    "require": "./lib/middleware.js",
    "import": "./lib/middleware.mjs"
  },
  "scripts": {
    "clean": "rimraf ./lib/",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "mocha --recursive test/unit/",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "prebuild": "run-s clean",
    "build": "rollup --config",
    "generate:md": "remark . --output",
    "preversion": "npm test",
    "prepack": "npm run build",
    "prepare": "husky install",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "milliseconds": "1.0.3",
    "timeout-as-promise": "1.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.28.3",
    "@rollup/plugin-babel": "6.1.0",
    "@travi/any": "3.1.3",
    "@travi/babel-preset": "3.0.146",
    "@travi/eslint-config": "1.0.86",
    "@travi/eslint-config-mocha": "1.0.25",
    "@travi/ioc": "1.1.1",
    "chai": "4.5.0",
    "chai-as-promised": "7.1.2",
    "commitlint-config-travi": "1.4.59",
    "cz-conventional-changelog": "3.3.0",
    "globstar": "1.0.0",
    "husky": "9.1.7",
    "ls-engines": "0.9.3",
    "mocha": "11.7.4",
    "npm-run-all2": "8.0.4",
    "publint": "0.3.15",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.8",
    "remark-toc": "9.0.0",
    "rollup": "4.52.5",
    "sinon": "21.0.0"
  },
  "peerDependencies": {
    "@travi/ioc": "^1.0.0"
  }
}
